{
    "manifest_version": 3,
    "name": "Student-Dashboard",
    "version": "1.0.0",
    "action": {
        "default_popup": "index.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "webNavigation","tabs","activeTab"
    ],
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ]
}